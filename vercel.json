{
  "rewrites": [
    {
      "source": "/chordcompass",
      "destination": "/index.html"
    },
    {
      "source": "/chordcompass/",
      "destination": "/index.html"
    },
    {
      "source": "/chordcompass/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/chordcompass/:path*",
      "destination": "/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/chordcompass/",
      "permanent": false
    }
  ]
}
