{
  "rewrites": [
    {
      "source": "/chordcompass/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/scalesage",
      "destination": "/scalesage.html"
    },
    {
      "source": "/scalesage/",
      "destination": "/scalesage.html"
    },
    {
      "source": "/scalesage/(.*)",
      "destination": "/scalesage.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/chordCompass",
      "destination": "/chordcompass/",
      "permanent": true
    },
    {
      "source": "/chordCompass/",
      "destination": "/chordcompass/",
      "permanent": true
    },
    {
      "source": "/chordCompass/:path*",
      "destination": "/chordcompass/:path*",
      "permanent": true
    }
  ]
}
